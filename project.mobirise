          "_name": "features15",
          "_sourceTheme": "hotelm4",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"mbr-section features15 hotelm4_features15\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n\t<mbr-parameters>\n\t\t\n\t\t<header>Size</header>\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n\t\t<input type=\"range\" inline title=\"Cards Count\" name=\"cardsAmount\" min=\"1\" max=\"3\" step=\"1\" value=\"2\">\n\t\t<input type=\"range\" inline title=\"1st Card Icons\" name=\"iconsCountFirst\" min=\"0\" max=\"5\" step=\"1\" value=\"4\">\n\t\t<input type=\"range\" inline title=\"2nd Card Icons\" name=\"iconsCountSecond\" min=\"0\" max=\"5\" step=\"1\" value=\"4\" condition=\"cardsAmount > 1\">\n\t\t<input type=\"range\" inline title=\"3rd Card Icons\" name=\"iconsCountThird\" min=\"0\" max=\"5\" step=\"1\" value=\"4\" condition=\"cardsAmount > 2\">\n\t\t<header>Show / Hide</header>\n\t\t<input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Underline\" name=\"showUnderline\" checked condition=\"showSubtitle\">\n\t\t<input type=\"color\" title=\"Underline Color\" name=\"underlineColor\" value=\"#878787\" condition=\"showUnderline && showSubtitle\">\n\t\t<input type=\"checkbox\" title=\"Scale Image On Hover\" name=\"scaleImg\" checked>\n\t\t<input type=\"checkbox\" title=\"Card Title\" name=\"showCardTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Info\" name=\"showInfo\" checked>\n\t\t<input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n\t\t<input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked>\n\t\t<input type=\"checkbox\" title=\"Borders\" name=\"showBorders\" checked>\n\t\t<input type=\"color\" title=\"Borders Color\" name=\"bordersColor\" value=\"#efefef\" condition=\"showBorders\">\n\t\t<input type=\"color\" title=\"Card BG Color\" name=\"cardBg\" value=\"#ffffff\">\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/helliniko-studios-1200x900.jpg\" parallax>\n\t\t\t<input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n\t\t\t<input type=\"video\" title=\"Video\" value=\"https://www.youtube.com/watch?v=2jqUN6d6ZyM\">\n\t\t</fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/hotelm4/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n\n\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n\t\t\n\t</mbr-parameters>\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n\t</div>\n\n\t<div class=\"container\">\n\t\t<h3 class=\"mbr-section-subtitle main-subtitle mbr-fonts-style mbr-lighter align-center\" mbr-class=\"{'pb-3': !showUnderline}\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".main-subtitle, .underline\">The nice and fully renovated rooms, the unique view to the traditional port of Naoussa and the warm hospitality, will make your stay memorable!</h3>\n\n\t\t<div class=\"underline align-center pb-3\" mbr-if=\"showUnderline && showSubtitle\">\n\t\t\t<div class=\"line\"></div>\n\t\t</div>\n\n\t\t<h2 class=\"mbr-section-title main-title mbr-fonts-style mbr-white align-center pb-5\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".main-title\">\n\t\t\tOur Rooms</h2>\n\t\t<div class=\"row justify-content-center\">\n\t\t\t<div class=\"card col-md-10\" mbr-class=\"{'col-lg-8': cardsAmount == 1,\n                            'col-lg-6': cardsAmount == 2,\n                            'col-lg-4': cardsAmount == 3}\">\n\t\t\t\t<div class=\"card-wrapper\">\n\t\t\t\t\t<div class=\"card-img\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/33869764-10215828830729032-9166253039152005120-n-1280x960.jpg\" alt=\"Mobirise\">\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-box\" mbr-if=\"showCardTitle || showInfo || showText || showButtons\">\n\t\t\t\t\t\t<h2 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title, .card-info\">Double Room with Side Sea View</h2>\n\t\t\t\t\t\t<div class=\"card-info\" mbr-class=\"{'pt-3': showCardTitle}\" mbr-if=\"showInfo\">\n\t\t\t\t\t\t\t<div class=\"info-wrapper\">\n\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbri-user\"></span>\n\t\t\t\t\t\t\t\t<p class=\"mbr-text info-text mbr-fonts-style mbr-lighter\" mbr-theme-style=\"display-4\" data-app-selector=\".info-text\">2 GUESTS</p>\n\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbri-home\"></span>\n\t\t\t\t\t\t\t\t<p class=\"mbr-text info-text mbr-fonts-style mbr-lighter\" mbr-theme-style=\"display-4\" data-app-selector=\".info-text\">20 sqm</p>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<p class=\"mbr-text card-text mbr-fonts-style mbr-lighter\" mbr-class=\"{'pt-3': showCardTitle || showInfo,\n                                    'm-0': !showButtons}\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".card-text, .card-btn\">Enjoying partial views over the Aegean Sea from its balcony, this air-conditioned room includes a fridge, a safe and a TV with satellite channels. Free Wi-Fi is available. Our rooms feature a super comfy double or twin beds and one built-in sofa-bed and choice of firm or soft pillows to guarantee you have a great night’s sleep.</p>\n\t\t\t\t\t\t<div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn card-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a href=\"https://ibe.channex.io/online-booking-despinas-mare-naoussa-paros-island\" class=\"btn btn-lg btn-info\" data-app-placeholder=\"Type Text\" target=\"_blank\">\n\t\t\t\t\t\t\t\tBOOK NOW</a></div>\n\t\t\t\t\t\t<div class=\"card-footer\" mbr-if=\"iconsCountFirst > 0\">\n\t\t\t\t\t\t\t<ul mbr-list mbr-list-grow=\"iconsCountFirst\" class=\"icons-list list-inline align-left\">\n\t\t\t\t\t\t\t\t<li class=\"list-inline-item\">\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbri-responsive\"></span>\n\t\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t\t\t<li class=\"list-inline-item\">\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbri-wifi\"></span>\n\t\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t<li class=\"list-inline-item\">\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont icon54-v1-air-conditioner\"></span>\n\t\t\t\t\t\t\t\t</li><li class=\"list-inline-item\">\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont sli-cup\"></span>\n\t\t\t\t\t\t\t\t</li></ul>\n\t\t\t\t\t\t\t<div class=\"link\">\n\t\t\t\t\t\t\t\t<div mbr-text class=\"link-text mbr-fonts-style mbr-lighter\" mbr-theme-style=\"display-4\" data-app-selector=\".link-text\"></div>\n\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbri-arrow-next\"></span>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\n\t\t\t<div class=\"card col-md-10\" mbr-class=\"{'col-lg-8': cardsAmount == 1,\n                            'col-lg-6': cardsAmount == 2,\n                            'col-lg-4': cardsAmount == 3}\" mbr-if=\"cardsAmount > 1\">\n\t\t\t\t<div class=\"card-wrapper\">\n\t\t\t\t\t<div class=\"card-img\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/33869764-10215828830729032-9166253039152005120-n-1280x960.jpg\" alt=\"Mobirise\">\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-box\" mbr-if=\"showCardTitle || showInfo || showText || showButtons\">\n\t\t\t\t\t\t<h2 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title, .card-info\">Double Room with Sea View</h2>\n\t\t\t\t\t\t<div class=\"card-info\" mbr-class=\"{'pt-3': showCardTitle}\" mbr-if=\"showInfo\">\n\t\t\t\t\t\t\t<div class=\"info-wrapper\">\n\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbri-user\"></span>\n\t\t\t\t\t\t\t\t<p class=\"mbr-text info-text mbr-fonts-style mbr-lighter\" mbr-theme-style=\"display-4\" data-app-selector=\".info-text\">2 GUESTS\n\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbri-home\"></span>\n\t\t\t\t\t\t\t\t<p class=\"mbr-text info-text mbr-fonts-style mbr-lighter\" mbr-theme-style=\"display-4\" data-app-selector=\".info-text\">38 sqm</p>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<p class=\"mbr-text card-text mbr-fonts-style mbr-lighter\" mbr-class=\"{'pt-3': showCardTitle || showInfo,\n                                'm-0': !showButtons}\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".card-text, .card-btn\">Enjoying unobstructed views over the Aegean Sea from its balcony, this air-conditioned room includes a fridge, a safe and a TV with satellite channels. Free Wi-Fi is available. Our rooms feature a super comfy double bed and choice of firm or soft pillows to guarantee you have a great night’s sleep.</p>\n\t\t\t\t\t\t<div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn card-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a href=\"https://ibe.channex.io/online-booking-despinas-mare-naoussa-paros-island\" class=\"btn btn-lg btn-info\" data-app-placeholder=\"Type Text\" target=\"_blank\">\n\t\t\t\t\t\t\t\tBOOK NOW</a></div>\n\t\t\t\t\t\t<div class=\"card-footer\" mbr-if=\"iconsCountSecond > 0\">\n\t\t\t\t\t\t\t<ul mbr-list mbr-list-grow=\"iconsCountSecond\" class=\"icons-list list-inline align-left\">\n\t\t\t\t\t\t\t\t<li class=\"list-inline-item\">\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbri-responsive\"></span>\n\t\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t\t\t<li class=\"list-inline-item\">\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbri-wifi\"></span>\n\t\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t<li class=\"list-inline-item\">\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont icon54-v1-air-conditioner\"></span>\n\t\t\t\t\t\t\t\t</li><li class=\"list-inline-item\">\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont sli-cup\"></span>\n\t\t\t\t\t\t\t\t</li></ul>\n\t\t\t\t\t\t\t<div class=\"link\">\n\t\t\t\t\t\t\t\t<div mbr-text class=\"link-text mbr-fonts-style mbr-lighter\" mbr-theme-style=\"display-4\" data-app-selector=\".link-text\"></div>\n\t\t\t\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont mbri-arrow-next\"></span>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\n\t\t\t<div class=\"card col-md-10\" mbr-class=\"{'col-lg-8': cardsAmount == 1,\n                            'col-lg-6': cardsAmount == 2,\n                            'col-lg-4': cardsAmount == 3}\" mbr-if=\"cardsAmount > 2\">\n\t\t\t\t<div class=\"card-wrapper\">\n\t\t\t\t\t<div class=\"card-img\">\n\t\t\t\t\t\t<img src=\"@PROJECT_PATH@/assets/images/scala-18-760x420.jpg\" alt=\"Mobirise\">\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"card-box\" mbr-if=\"showCardTitle || showInfo || showText || showButtons\">\n\t\t\t\t\t\t<h2 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title, .card-info\">Simple Double Room</h2>\n\t\t\t\t\t\t<di